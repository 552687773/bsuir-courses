<div class="show">
  <p id="notice"><%= notice %></p>

  <p>
    <strong>Title:</strong>
    <%= @post.title %>
  </p>

  <p>
    <strong>Text:</strong>
    <%= @post.text %>
  </p>

  <p>
    <strong>Contact:</strong>
    <%= @post.contact %>
  </p>

  <%= link_to 'Edit', edit_post_path(@post) %> |
  <%= link_to 'Back', posts_path %>
</div>
