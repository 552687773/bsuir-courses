<%= simple_form_for(@b2_b) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :title %>
    <%= f.input :body %>
    <%= f.input :phone %>
    <%= f.input :name %>
    <%= f.input :sell_currency %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
